@model Party

<div class="col-lg-7 mb-4 mb-sm-0">
    <img alt="Image" src="@Model.Image" class="rounded img-fluid"/>
    <li class="breadcrumb-item active mt-3" aria-current="page">@Model.PartyClubCategory Party in @Model.Address.City, @Model.Address.Province</li>

<div class="text-justify mt-3">
    <h1 class="mb-2">@Model.Title</h1>
    <h2 class="ml-5 mt-2">@Model.Description</h2>
    <h1 class="mb-2">@Model.EntryFee</h1>
    <h3 class="mb-2">@Model.Contact</h3>
    <h3 class="mb-2">@Model.Website</h3>
    <h3 class="mb-2">@Model.Facebook</h3>
    <h3 class="font-weight-light mt-6">@Model.Address.City, @Model.Address.Province</h3>
    <h3 class="font-weight-light mt-2">@Model.PartyClubCategory </h3>
</div>


